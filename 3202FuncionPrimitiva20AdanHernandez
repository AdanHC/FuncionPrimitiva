clear all
clc
syms x
f=input('Ingresar funcion: ')
a=input('Ingresar intervalo inferior: ')
b=input('Ingresar intervalo superior: ')
integral=int(f,x,a,b)

x=-4:0.1:2;
y=abs(x+2);
plot(x,y)
title("f(x)=│x+2│");
xlabel("X");
ylabel("Y");
